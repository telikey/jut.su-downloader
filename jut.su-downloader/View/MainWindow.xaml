<Window x:Class="jut.su_downloader.View.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:jut.su_downloader.View"
        xmlns:commandConverter="clr-namespace:WPFCommands;assembly=WPFCommands"
        mc:Ignorable="d"
        Height="450" Width="800">
    <Window.Resources>
        <commandConverter:StringToCommandConverter x:Key="StringToCommandConverter"></commandConverter:StringToCommandConverter>
    </Window.Resources>
    <Grid>
        <Button Command="{Binding MainWindowCommands, Converter={StaticResource StringToCommandConverter}, ConverterParameter='button1_Command'}"></Button>
    </Grid>
</Window>
